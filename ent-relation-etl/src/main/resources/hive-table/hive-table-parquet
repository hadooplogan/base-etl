CREATE EXTERNAL TABLE e_inv_investment_parquet(
       s_ext_nodenum string,
       pripid string,
       s_ext_sequence string,
       invid string,
       inv string,
       invtype string,
       certype string,
       cerno string,
       blictype string,
       blicno string,
       country string,
       currency string,
       subconam string,
       acconam string,
       subconamusd string,
       acconamusd string,
       conprop string,
       conform string,
       condate string,
       baldelper string,
       conam string,
       exeaffsign string,
       s_ext_timestamp string,
       s_ext_batch string,
       s_ext_validflag string,
       linkman string,
       cerno_old string,
       subconam_new string,
       conprop_new string,
       status string,
       record_stat string,
       record_desc string,
       match string,
       zspid string)
ROW FORMAT SERDE 'parquet.hive.serde.ParquetHiveSerDe'
STORED AS  PARQUET
LOCATION '/relation/cachetable/e_inv_investment_parquet';


